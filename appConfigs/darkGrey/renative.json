{
    "id": "darkGrey",
    "common": {
        "id": "com.reactseals.template.chat",
        "title": "chat",
        "description": "Cross-platform application project based on ReNative",
        "author": {
            "name": "ReactSeals",
            "email": "ReactSeals <info@reactseals.com>",
            "url": "https://github.com/reactseals"
        },
        "license": "MIT",
        "includedPlugins": ["*"],
        "includedFonts": ["*"],
        "defaultSchemes": {
            "run": "debug",
            "build": "release",
            "export": "release"
        }
    },
    "platforms": {
        "ios": {
            "entryFile": "index.ios",
            "scheme": "RNVApp",
            "teamID": "PRNK99PXGZ",
            "provisioningStyle": "Automatic",
            "backgroundColor": "black",
            "systemCapabilities": {
                "com.apple.AccessWiFi": false,
                "com.apple.ApplePay": false,
                "com.apple.ApplicationGroups.iOS": false,
                "com.apple.AutoFillCredentialProvider": false,
                "com.apple.BackgroundModes": false,
                "com.apple.ClassKit": false,
                "com.apple.DataProtection": false,
                "com.apple.GameCenter.iOS": false,
                "com.apple.HealthKit": false,
                "com.apple.HomeKit": false,
                "com.apple.HotspotConfiguration": false,
                "com.apple.InAppPurchase": false,
                "com.apple.InterAppAudio": false,
                "com.apple.Keychain": false,
                "com.apple.Maps.iOS": false,
                "com.apple.Multipath": false,
                "com.apple.NearFieldCommunicationTagReading": false,
                "com.apple.NetworkExtensions.iOS": false,
                "com.apple.Push": false,
                "com.apple.SafariKeychain": false,
                "com.apple.Siri": false,
                "com.apple.VPNLite": false,
                "com.apple.WAC": false,
                "com.apple.Wallet": false,
                "com.apple.iCloud": false
            },
            "entitlements": {},
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "id": "com.reactseals.template.chat.debug",
                    "runScheme": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "runScheme": "Release",
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            },
            "exportOptions": {
                "method": "app-store",
                "uploadBitcode": true,
                "compileBitcode": false,
                "uploadSymbols": true,
                "signingStyle": "automatic",
                "signingCertificate": "iPhone Distribution"
            }
        },
        "android": {
            "entryFile": "index.android",
            "universalApk": true,
            "multipleAPKs": false,
            "minSdkVersion": 21,
            "backgroundColor": "black",
            "permissions": ["*"],
            "buildSchemes": {
                "debug": {
                    "id": "com.reactseals.template.chat.debug",
                    "signingConfig": "Debug",
                    "bundleAssets": false
                },
                "release": {
                    "signingConfig": "Release",
                    "multipleAPKs": true,
                    "bundleAssets": true,
                    "bundleIsDev": true
                }
            }
        },
        "macos": {
            "entryFile": "index.macos",
            "backgroundColor": "black",
            "buildSchemes": {
                "debug": {
                    "id": "com.reactseals.template.chat.debug",
                    "bundleAssets": false
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false
                }
            }
        },
        "web": {
            "entryFile": "index.web",
            "backgroundColor": "black",
            "buildSchemes": {
                "debug": {
                    "id": "com.reactseals.template.chat.debug",
                    "bundleAssets": false,
                    "environment": "development"
                },
                "release": {
                    "bundleAssets": true,
                    "bundleIsDev": false,
                    "environment": "production"
                }
            }
        }
    },
    "runtime": {
        "theme": {
            "backgroundColor": "#2c2c2e",
            "activeBackgroundColor": "#636366",
            "activeColorPrimary": "#838388",
            "activeColorSecondary": "#3a3a3c",
            "activeColorTertiary": "#48484a",
            "textColor": "#edf2f4",
            "userNicknameColor": "#dfbf4c",
            "userMessageBackground": "#222222",
            "nicknameColor": "#58a3ad",
            "messageBackground": "#b4b4b4",
            "chatTextColor": "#222222",
            "buttonTextColor": "#edf2f4"
        },
        "fonts": {
            "GillSans": "Gill Sans",
            "Optima": "Optima",
            "Roboto": "Roboto"
        }
    }
}
