{
  "id": "helloWorld",
  "common": {
    "id": "com.mycompany.chat",
    "title": "chat",
    "description": "Cross-platform application project based on ReNative",
    "author": {
      "name": "Pavel Jacko",
      "email": "Pavel Jacko <i@pavjacko.com>",
      "url": "https://github.com/pavjacko"
    },
    "license": "MIT",
    "includedPlugins": [
      "*"
    ],
    "includedFonts": [
      "*"
    ],
    "defaultSchemes": {
      "run": "debug",
      "build": "release",
      "export": "release"
    }
  },
  "platforms": {
    "ios": {
      "entryFile": "index.ios",
      "scheme": "RNVApp",
      "teamID": "",
      "provisioningStyle": "Manual",
      "backgroundColor": "black",
      "systemCapabilities": {
        "com.apple.AccessWiFi": false,
        "com.apple.ApplePay": false,
        "com.apple.ApplicationGroups.iOS": false,
        "com.apple.AutoFillCredentialProvider": false,
        "com.apple.BackgroundModes": false,
        "com.apple.ClassKit": false,
        "com.apple.DataProtection": false,
        "com.apple.GameCenter.iOS": false,
        "com.apple.HealthKit": false,
        "com.apple.HomeKit": false,
        "com.apple.HotspotConfiguration": false,
        "com.apple.InAppPurchase": false,
        "com.apple.InterAppAudio": false,
        "com.apple.Keychain": false,
        "com.apple.Maps.iOS": false,
        "com.apple.Multipath": false,
        "com.apple.NearFieldCommunicationTagReading": false,
        "com.apple.NetworkExtensions.iOS": false,
        "com.apple.Push": false,
        "com.apple.SafariKeychain": false,
        "com.apple.Siri": false,
        "com.apple.VPNLite": false,
        "com.apple.WAC": false,
        "com.apple.Wallet": false,
        "com.apple.iCloud": false
      },
      "entitlements": {},
      "permissions": [
        "*"
      ],
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "runScheme": "Debug",
          "bundleAssets": false
        },
        "release": {
          "runScheme": "Release",
          "bundleAssets": true,
          "bundleIsDev": false
        }
      }
    },
    "android": {
      "entryFile": "index.android",
      "universalApk": true,
      "multipleAPKs": false,
      "minSdkVersion": 21,
      "backgroundColor": "black",
      "permissions": [
        "*"
      ],
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "signingConfig": "Debug",
          "bundleAssets": false
        },
        "release": {
          "signingConfig": "Release",
          "multipleAPKs": true,
          "bundleAssets": true,
          "bundleIsDev": true
        }
      }
    },
    "macos": {
      "entryFile": "index.macos",
      "backgroundColor": "black",
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "bundleAssets": false
        },
        "release": {
          "bundleAssets": true,
          "bundleIsDev": false
        }
      }
    },
    "web": {
      "entryFile": "index.web",
      "backgroundColor": "black",
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "bundleAssets": false,
          "environment": "development"
        },
        "release": {
          "bundleAssets": true,
          "bundleIsDev": false,
          "environment": "production"
        }
      }
    }
  }
}