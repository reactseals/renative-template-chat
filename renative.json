{
    "projectName": "@reactseals/renative-template-chat",
    "defaults": {
        "supportedPlatforms": ["ios", "android", "web", "macos"],
        "defaultAppId": "com.reactseals.template.chat",
        "ports": {}
    },
    "currentTemplate": "renative-template-hello-world",
    "templates": {
        "renative-template-hello-world": {
            "version": "0.31.3"
        }
    },
    "paths": {
        "globalConfigDir": "~/.rnv",
        "appConfigsDir": "./appConfigs",
        "platformTemplatesDir": "RNV_HOME/platformTemplates",
        "entryDir": "./",
        "platformAssetsDir": "./platformAssets",
        "platformBuildsDir": "./platformBuilds",
        "projectConfigDir": "./projectConfig",
        "appConfigDirs": [null]
    },
    "platforms": {
        "android": {
            "gradle.properties": {
                "android.useDeprecatedNdk": true
            },
            "AndroidManifest": {
                "children": [
                    {
                        "tag": "application",
                        "android:name": ".MainApplication",
                        "android:usesCleartextTraffic": true,
                        "tools:targetApi": 26,
                        "children": [
                            {
                                "tag": "activity",
                                "android:name": ".MainActivity",
                                "android:screenOrientation": "portrait",
                                "android:noHistory": "false",
                                "android:launchMode": "singleTask"
                            }
                        ]
                    }
                ]
            }
        },
        "web": {
            "engine": "engine-rn-next"
        }
    },
    "plugins": {
        "renative": "source:rnv",
        "react": "source:rnv",
        "react-art": "source:rnv",
        "react-dom": "source:rnv",
        "react-native": "source:rnv",
        "react-native-web": "source:rnv",
        "react-native-web-image-loader": "source:rnv",
        "react-native-reanimated": "source:rnv",
        "react-native-vector-icons": "source:rnv",
        "react-native-linear-gradient": "source:rnv",
        "@react-navigation": "source:rnv",
        "react-native-screens": "source:rnv",
        "react-native-safe-area-context": "source:rnv",
        "react-native-safe-area-view": "source:rnv",
        "@react-native-community/masked-view": "source:rnv",
        "next": "source:rnv",
        "react-native-gesture-handler": {
            "version": "1.6.0"
        },
        "react-native-dotenv": "2.4.3",
        "react-native-event-listeners": "1.0.7",
        "recyclerlistview": "3.0.0",
        "@reach/router": "1.3.4",
        "@react-native-firebase/app": "source:rnv",
        "@react-native-firebase/auth": {
            "version": "9.0.0",
            "dependsOn": ["@react-native-firebase/app"],
            "ios": {
                "isStatic": true,
                "podName": "RNFBAuth",
                "xcodeproj": {
                    "resourceFiles": ["RNVApp/GoogleService-Info.plist"]
                },
                "appDelegateImports": ["Firebase"],
                "appDelegateMethods": {
                    "application": {
                        "didFinishLaunchingWithOptions": ["FirebaseApp.configure()"]
                    }
                }
            },
            "android": {
                "package": "io.invertase.firebase.auth.ReactNativeFirebaseAuthPackage",
                "projectName": "@react-native-firebase_auth"
            }
        },
        "@react-native-firebase/database": {
            "version": "7.3.2",
            "dependsOn": ["@react-native-firebase/app"],
            "android": {
                "package": "io.invertase.firebase.database.ReactNativeFirebaseDatabasePackage",
                "projectName": "@react-native-firebase_database"
            },
            "ios": {
                "isStatic": true,
                "podName": "RNFBDatabase",
                "xcodeproj": {
                    "resourceFiles": ["RNVApp/GoogleService-Info.plist"]
                },
                "appDelegateImports": ["Firebase"],
                "appDelegateMethods": {
                    "application": {
                        "didFinishLaunchingWithOptions": ["FirebaseApp.configure()"]
                    }
                }
            }
        }
    },
    "permissions": {
        "ios": {
            "NSPhotoLibraryAddUsageDescription": {
                "key": "NSPhotoLibraryAddUsageDescription",
                "desc": "Need library to save images"
            },
            "NSPhotoLibraryUsageDescription": {
                "key": "NSPhotoLibraryUsageDescription",
                "desc": "Allows to upload images from photo library"
            }
        },
        "android": {
            "INTERNET": {
                "key": "android.permission.INTERNET",
                "security": "normal"
            },
            "SYSTEM_ALERT_WINDOW": {
                "key": "android.permission.SYSTEM_ALERT_WINDOW",
                "security": "signature"
            }
        }
    }
}
