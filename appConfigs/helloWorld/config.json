{
  "id": "helloWorld",
  "common": {
    "id": "com.mycompany.chatnew",
    "title": "Chat",
    "description": "Cross-platform application project based on ReNative",
    "author": {
      "name": "Pavel Jacko",
      "email": "Pavel Jacko <i@pavjacko.com>",
      "url": "https://github.com/pavjacko"
    },
    "license": "MIT",
    "includedPlugins": [
      "*"
    ],
    "includedFonts": [
      "*"
    ],
    "defaultSchemes": {
      "run": "debug",
      "build": "release",
      "export": "release"
    }
  },
  "platforms": {
    "ios": {
      "entryFile": "index.ios",
      "scheme": "RNVApp",
      "teamID": "",
      "backgroundColor": "black",
      "permissions": [
        "*"
      ],
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "runScheme": "Debug",
          "bundleAssets": false
        },
        "release": {
          "runScheme": "Release",
          "bundleAssets": true,
          "bundleIsDev": false
        }
      }
    },
    "android": {
      "entryFile": "index.android",
      "universalApk": true,
      "multipleAPKs": false,
      "minSdkVersion": 21,
      "backgroundColor": "black",
      "permissions": [
        "*"
      ],
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "signingConfig": "Debug",
          "bundleAssets": false
        },
        "release": {
          "signingConfig": "Release",
          "multipleAPKs": true,
          "bundleAssets": true,
          "bundleIsDev": true
        }
      }
    },
    "macos": {
      "entryFile": "index.macos",
      "backgroundColor": "black",
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "bundleAssets": false
        },
        "release": {
          "bundleAssets": true,
          "bundleIsDev": false
        }
      }
    },
    "web": {
      "entryFile": "index.web",
      "backgroundColor": "black",
      "buildSchemes": {
        "debug": {
          "id": "renative.helloworld.debug",
          "bundleAssets": false,
          "environment": "development"
        },
        "release": {
          "bundleAssets": true,
          "bundleIsDev": false,
          "environment": "production"
        }
      }
    }
  }
}